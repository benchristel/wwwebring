  "main": "dist/wwwebring.js",
  "repository": "https://github.com/benchristel/wwwebring",
  "author": "Ben Christel",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@benchristel/taste": "^0.5.0",
    "glob": "^10.4.1",
    "typescript": "^5.4.5",
    "vite": "^5.2.12"
  },
  "scripts": {
    "dev": "yarn vite",
    "ts": "yarn tsc --watch --noEmit",
